#include <stdio.h>

int main() {
    int n, m, i, j, k;
    int page[50], frame[10];
    int flag, count = 0, pointer = 0;

    printf("Enter number of pages: ");
    scanf("%d", &n);

    printf("Enter page reference sequence:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &page[i]);
    }

    printf("Enter number of frames: ");
    scanf("%d", &m);

    // Initialize frames
    for (i = 0; i < m; i++)
        frame[i] = -1;

    printf("\nPage\tFrames\tPage Fault\n");
    for (i = 0; i < n; i++) {
        flag = 0;

        // Check if page is already in frame
        for (j = 0; j < m; j++) {
            if (frame[j] == page[i]) {
                flag = 1; // hit
                break;
            }
        }

        if (flag == 0) {
            // Page fault, replace using FIFO
            frame[pointer] = page[i];
            pointer = (pointer + 1) % m;
            count++;
            printf("%d\t", page[i]);
            for (k = 0; k < m; k++)
                printf("%d ", frame[k]);
            printf("\tYes\n");
        } else {
            printf("%d\t", page[i]);
            for (k = 0; k < m; k++)
                printf("%d ", frame[k]);
            printf("\tNo\n");
        }
    }

    printf("\nTotal Page Fau
